<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">

		<title>
			{% block title %} Haras de la Cense {% endblock %}
		
	
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		<link "https://bootswatch.com/5/lux/bootstrap.min.css">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		<!-- CSS only -->
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous"> 
		{% block stylesheets %}
			{# {{ encore_entry_link_tags('app') }} #}
		{% endblock %}
		<link rel="stylesheet" href="{{asset ('css/lightbox.min.css')}}">
		<link rel="stylesheet" href="{{asset ('css/frontend.css')}}">
	</head>
	<body>

			<table width="100%">
				<tr>
					<td>
					<a href="{{path('app_accueil')}}">
					<img src="{{ asset('images/evenement/Logoharascense3.jpg') }}" alt="logo haras de la cense"  height="100px">
					</a>
					</td>
					<br><br>
					<td>
						<div class="logout">
						{% if app.user %}
							<a href="{{path('app_logout')}}" class="logout">Deconnection</a>
							{% else %}
							<a href="{{path('app_login')}}" class="logout">Connection</a>
							{% endif %}
						</div>
						<h1 class="intro">Le Site des Propriétaires du Haras de la Cense - Bretigny </h1>
	<br><br>			
					</td>
				</tr>
			</table>

			<nav class="navbar navbar-expand-lg navbar-dark bg-dark  container-fluid px-0">
				<div class="container-fluid">
					<div class="collapse navbar-collapse" id="navbarColor01">
						<ul class="navbar-nav me-auto">
						{% if app.user %}
						{% else %}
							<li class="nav-item">
								<a class="nav-link " style="font-size: 24px;" href="{{ path('app_register')}}">Inscription</a>
							</li>
						{% endif %}
						{% if app.user %}
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_user')}}">Espace-Membre</a>
							</li>
						{% endif %}
						
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_front_user_index')}}">Propriétaires</a> 
							</li>
							<li class="nav-item">
								<a class="nav-link "  style="font-size: 24px;" href="{{ path('app_cavalier')}}">Cavaliers</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_cheval')}}">Chevaux</a>
							</li>
							{% if app.user %}
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_ballade')}}">Ballades</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_service')}}">Services</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_covoit')}}">Co-Voiturage</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('workingforum_forum')}}">Forum</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_evenement')}}">Evenements</a>
							</li>
							{% endif %}
							<li class="nav-item">
								<a class="nav-link" style="font-size: 24px;" href="{{ path('app_reglement')}}">Reglement</a>
							</li>
						</ul>
						{# <form class="d-flex">
							<input class="form-control me-sm-2" type="text" placeholder="Search">
							<button class="btn btn-secondary my-2 my-sm-0" type="submit">
								Chercher</button>
						</form> #}
					</div>
				</div>
			</nav>
	</div>
	<div class="container-fluid">
	{% block body %}{% endblock %}
	{% block forum %}{% endblock %}
	</div>	
	<footer class= "container-fluid footer px-0" >
		<nav class="navbar navbar-expand-lg navbar-dark bg-dark container-fluid">
				<div class="collapse navbar-collapse" id="navbarColor01">
					<ul class="navbar-nav me-auto">
						<li class="nav-item">
						<a class="nav-link active" href="#">
							<span class="visually-hidden">(current)</span>
						</a>
						</li>
						<li class="nav-item">
						<a class="nav-link" style="font-size: 24px;" href="{{path('app_contact')}}">Contact</a>
						</li>
						<li class="nav-item">
						<a class="nav-link" style="font-size: 24px;" href="#">Plan de la Cense</a>
						</li>
						<li class="nav-item">
						<a class="nav-link" style="font-size: 24px;" href="#">Plan de la Forêt de Rambouillet</a>
						</li>
					</ul>
				</div>
		</nav>
	</footer>


	<!-- JavaScript Bundle with Popper 
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>-->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
	<script src="{{asset('js/app.js')}}"></script>
	<script src="{{asset('js/lightbox.min.js')}}"></script>
	{% block javascripts %}
		{{ encore_entry_script_tags('app') }}
	{% endblock %}


	</body>
</html>
