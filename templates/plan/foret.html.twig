{% extends 'base.html.twig' %}

{% block title %}Haras de la Cense - Le Plan de la Forêt{% endblock %}
{% block body %}
<style>
#map {
  height: 400px;
  /* The height is 400 pixels */
  width: 100%;
  /* The width is the width of the web page */
  /* https://console.cloud.google.com/apis/credentials?project=floharascense AIzaSyAKyeL4db_5Wn2iCoZNuCUrALk1djKrzXA */
}
</style>
<div class="example-wrapper">
    <h1 class="ptitre" >La Forêt de Rambouillet</h1>
    <div id="map">
    </div>
</div>
<script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAKyeL4db_5Wn2iCoZNuCUrALk1djKrzXA&callback=initMap&v=weekly"
    defer>
</script>
<script>
// Initialize and add the map
function initMap() {
  // The location of Uluru
  const foret = { lat: 48.667062618102776, lng: 1.8332903538496244};
  // The map, centered at La Cense
  const map = new google.maps.Map(document.getElementById("map"), {
    zoom: 11,
    center: foret,
  });
  // The marker, positioned at Uluru
  const marker = new google.maps.Marker({
    position: foret,
    map: map,
  });
}
window.initMap = initMap;
</script>

{% endblock %}
