  // H2W4+RF Rochefort-en-Yvelines 
  // The map, centered at La Cense
  const map = new google.maps.Map(document.getElementById("foret"), {
    zoom: 14,
    center: cense,
  });
  // The marker, positioned at Uluru
  const marker = new google.maps.Marker({
    position: cense,
    map: map,
  });
}
window.initMap = initMap;
