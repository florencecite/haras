{% extends 'base.html.twig' %}

{% block title %}La liste des propriétaires{% endblock %}

{% block body %}
    <h1>la listes des propriétaires</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Email</th>
                <th>Telephone</th>
                {# <th>Username</th>
            </tr> #}
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                {# <td>{{ user.roles ? user.roles|json_encode : '' }}</td> #}
                <td>{{ user.nom }}</td>
                <td>{{ user.prenom }}</td>
                <td>{{ user.email }}</td>
                <td>{{ user.telephone }}</td>
                <td>{{ user.username }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">Aucun Propriétaire</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
